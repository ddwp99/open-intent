{
  "version": 1,
  "entities": {
    "@waiter":
    {
      "Bob": ["Bobby"]
    },
    "@beverage":
    {
      "Coca-Cola": ["Coca", "Cola", "Coke"],
      "Kronenbourg": ["Kro"],
      "Heineken": ["Hein"],
      "Café": [],
      "Arabica": [],
      "Mocha": [],
      "Eau de vie": [],
      "Eau de Zilia": [] 
    },
    "@number":
    {
      "0": ["zero"],
      "1": ["un", "une"],
      "2": ["deux"],
      "3": ["trois"],
      "4": ["quatre"],
      "5": ["cinq"],
      "6": ["six"],
      "7": ["sept"],
      "8": ["huit"],
      "9": ["neuf"]
    },
    "@yes":
    {
      "oui": []
    },
    "@no":
    {
      "non": []
    },
    "@nothing":
    {
      "rien": []
    }
  },


  "intents": [
    {
      "id": "order1",
      "intent": [
        "@number",
        "@beverage"
      ],
      "example": "Je veux 1 Coca."
    },
    {
      "id": "order2",
      "intent": [
        "@number",
        "@beverage",
        "@number",
        "@beverage"
      ],
      "example": "Je veux 1 Coca et 1 bière"
    },
    {
      "id": "yes",
      "intent": ["@yes"],
      "example": "Oui"
    },
    {
      "id": "no",
      "intent": ["@no"],
      "example": "Non"
    },
    {
      "id": "nothing",
      "intent": ["@nothing"],
      "example": "Rien"
    },
    {
      "id": "ask_waiter",
      "intent": ["@waiter"],
      "example": "Bob!"
    }
  ],

  "intent_story": {
    "root": "init",
    "graph": {
      "init": {
        "ask_waiter": "wait_order"
      },
      "wait_order": {
        "order1:append_order1": "wait_another_order",
        "order2:append_order2": "wait_another_order",
        "nothing": "init"
      },
      "wait_another_order": {
        "order1:append_order1": "wait_another_order",
        "order2:append_order2": "wait_another_order",
        "nothing": "init"
      }
    }
  }
}

